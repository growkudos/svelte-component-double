{
  "name": "@growkudos/svelte-component-double",
  "version": "3.0.0-alpha.2",
  "description": "A test double for Svelte 5 components",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run prepack",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "prepack": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./vitest": "./dist/matchers/vitest.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growkudos/svelte-component-double.git"
  },
  "keywords": [
    "svelte"
  ],
  "author": "Kudos Innovations Ltd",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/growkudos/svelte-component-double/issues"
  },
  "homepage": "https://github.com/growkudos/svelte-component-double#readme",
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "optionalPeerDependencies": {
    "vitest": ">= 0.25"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^6.0.0",
    "@sveltejs/kit": "^2.22.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^6.0.0",
    "publint": "^0.3.2",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^7.0.4"
  },
  "volta": {
    "node": "22.19.0"
  }
}
